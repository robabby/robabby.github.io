{
  "name": "robabby.com",
  "description": "My website",
  "version": "1.0.0",
  "author": "Rob Abby ( robabby23@gmail.com )",
  "keywords": [
    "gatsby, typescript, sass, docker, eslint, responsive"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "devLocal": "concurrently --kill-others \"gatsby develop\" \"npm run watch\"",
    "devDocker": "concurrently --kill-others \"gatsby develop -H 0.0.0.0\" \"SHELL=/bin/bash npm run watch\"",
    "watch": "chokidar \"**/*.scss\" -c \"npm run buildScssTypings\"",
    "fix": "prettier --write 'src/**/*.{ts,tsx,scss,md}' && eslint --fix 'src/**/*.{ts,tsx}' && stylelint --fix 'src/**/*.scss' ",
    "buildScssTypings": "node scripts/generateTypings.js",
    "generate": "plop --plopfile ./generators/plopfile.js",
    "graphql-types": "graphql-codegen --config ./graphql-config.yml",
    "dev": "gatsby develop",
    "serve": "gatsby serve -p 9000 -o",
    "clean": "gatsby clean",
    "deploy": "gatsby clean && gatsby build && gh-pages -d public -b master"
  },
  "devDependencies": {
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-helmet": "^6.1.5",
    "@typescript-eslint/eslint-plugin": "5.13.0",
    "@typescript-eslint/parser": "5.13.0",
    "chokidar-cli": "3.0.0",
    "concurrently": "4.1.1",
    "eslint": "8.10.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.4.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.29.2",
    "gatsby-plugin-sass": "^5.8.0",
    "gatsby-source-filesystem": "^4.8.1",
    "gatsby-transformer-remark": "^5.8.1",
    "gh-pages": "^2.1.1",
    "husky": "3.0.1",
    "lint-staged": "9.2.0",
    "mkdirp": "^1.0.4",
    "node-gyp": "^8.4.1",
    "node-sass-glob-importer": "5.3.2",
    "plop": "^3.0.5",
    "prettier": "2.5.1",
    "prettier-tslint": "0.4.2",
    "remark-cli": "^10.0.1",
    "remark-frontmatter": "^4.0.1",
    "remark-preset-lint-recommended": "^6.1.2",
    "stylelint": "^14.5.3",
    "stylelint-config-css-modules": "^3.0.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-scss": "^5.0.2",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.1.0",
    "ts-node": "10.5.0",
    "tslint": "^6.1.3",
    "tslint-config-blvd": "^1.2.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-loader": "3.5.4",
    "tslint-plugin-prettier": "^2.3.0",
    "tslint-react": "5.0.0",
    "typed-css-modules": "0.7.0",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@chakra-ui/gatsby-plugin": "^2.0.4",
    "@chakra-ui/react": "^1.8.6",
    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "@graphql-codegen/add": "^3.1.1",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript": "^2.4.5",
    "change-case": "^4.1.2",
    "emotion-theming": "^11.0.0",
    "framer-motion": "^6.2.8",
    "gatsby": "^4.8.1",
    "gatsby-link": "^4.8.1",
    "gatsby-plugin-google-analytics": "^4.8.0",
    "gatsby-plugin-react-helmet": "^5.8.0",
    "gatsby-plugin-root-import": "2.0.8",
    "gatsby-plugin-sharp": "4.8.1",
    "gatsby-plugin-typescript": "4.8.1",
    "gatsby-remark-autolink-headers": "^5.8.0",
    "gatsby-remark-copy-linked-files": "^5.8.0",
    "gatsby-remark-images": "6.8.1",
    "gatsby-remark-prismjs": "^6.8.0",
    "gatsby-transformer-json": "^4.8.0",
    "gatsby-transformer-sharp": "4.8.0",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.3.1",
    "react-particles-js": "^3.6.0",
    "react-three-fiber": "^6.0.13",
    "sass": "^1.49.9",
    "three": "^0.138.1"
  }
}
