{
  "name": "robabby.com",
  "description": "My website",
  "version": "1.0.0",
  "author": "Rob Abby ( robabby23@gmail.com )",
  "keywords": [
    "gatsby, typescript, sass, docker, eslint, responsive"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "devLocal": "concurrently --kill-others \"gatsby develop\" \"npm run watch\"",
    "devDocker": "concurrently --kill-others \"gatsby develop -H 0.0.0.0\" \"SHELL=/bin/bash npm run watch\"",
    "watch": "chokidar \"**/*.scss\" -c \"npm run buildScssTypings\"",
    "fix": "prettier --write 'src/**/*.{ts,tsx,scss,md}' && eslint --fix 'src/**/*.{ts,tsx}' && stylelint --fix 'src/**/*.scss' ",
    "buildScssTypings": "node scripts/generateTypings.js",
    "generate": "plop --plopfile ./generators/plopfile.js",
    "graphql-types": "graphql-codegen --config ./graphql-config.yml",
    "dev": "gatsby dev",
    "serve": "gatsby serve -p 9000 -o",
    "clean": "gatsby clean",
    "deploy": "gatsby clean && gatsby build && gh-pages -d public -b master"
  },
  "devDependencies": {
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/react-helmet": "^5.0.8",
    "@typescript-eslint/eslint-plugin": "2.13.0",
    "@typescript-eslint/parser": "2.13.0",
    "chokidar-cli": "1.2.2",
    "concurrently": "4.1.1",
    "eslint": "6.0.1",
    "eslint-config-airbnb": "17.1.1",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.14.2",
    "gatsby-plugin-sass": "^2.1.3",
    "gatsby-source-filesystem": "^2.1.5",
    "gatsby-transformer-remark": "^2.6.7",
    "gh-pages": "^2.1.1",
    "husky": "3.0.1",
    "lint-staged": "9.2.0",
    "mkdirp": "^0.5.1",
    "node-gyp": "5.0.3",
    "node-sass": "^4.12.0",
    "node-sass-glob-importer": "5.3.2",
    "plop": "^2.5.3",
    "prettier": "1.18.2",
    "prettier-tslint": "0.4.2",
    "remark-cli": "^7.0.1",
    "remark-frontmatter": "^1.3.2",
    "remark-preset-lint-recommended": "^3.0.3",
    "stylelint": "^10.1.0",
    "stylelint-config-css-modules": "^1.4.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-recommended-scss": "^3.3.0",
    "stylelint-order": "^3.0.1",
    "stylelint-scss": "^3.9.2",
    "ts-node": "8.3.0",
    "tslint": "^5.18.0",
    "tslint-config-blvd": "^1.2.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-loader": "3.5.4",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "4.0.0",
    "typed-css-modules": "0.3.7",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "@chakra-ui/core": "^0.5.2",
    "@emotion/core": "^10.0.27",
    "@emotion/styled": "^10.0.27",
    "@graphql-codegen/add": "^1.9.1",
    "@graphql-codegen/cli": "^1.9.1",
    "@graphql-codegen/typescript": "^1.9.1",
    "change-case": "^4.1.1",
    "emotion-theming": "^10.0.27",
    "framer": "^1.1.6",
    "framer-motion": "^1.7.0",
    "gatsby": "^2.13.31",
    "gatsby-link": "^2.2.27",
    "gatsby-plugin-chakra-ui": "^0.1.2",
    "gatsby-plugin-google-analytics": "^2.1.31",
    "gatsby-plugin-react-helmet": "^3.1.2",
    "gatsby-plugin-root-import": "2.0.5",
    "gatsby-plugin-sharp": "2.2.8",
    "gatsby-plugin-transition-link": "^1.17.7",
    "gatsby-plugin-typescript": "2.1.2",
    "gatsby-remark-autolink-headers": "^2.1.21",
    "gatsby-remark-copy-linked-files": "^2.1.33",
    "gatsby-remark-images": "3.1.6",
    "gatsby-remark-prismjs": "^3.3.28",
    "gatsby-transformer-json": "^2.2.22",
    "gatsby-transformer-sharp": "2.2.4",
    "gsap": "^3.0.5",
    "lodash": "^4.17.15",
    "prismjs": "^1.17.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-icons": "^3.8.0",
    "react-particles-js": "^2.7.0",
    "react-three-fiber": "^3.0.17",
    "three": "^0.137.0"
  }
}
